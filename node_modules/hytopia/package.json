{
  "name": "hytopia",
  "version": "0.10.13",
  "description": "The HYTOPIA SDK makes it easy for developers to create massively multiplayer games using JavaScript or TypeScript.",
  "type": "module",
  "main": "./server.mjs",
  "exports": {
    ".": {
      "types": "./server.d.ts",
      "default": "./server.mjs"
    }
  },
  "bin": {
